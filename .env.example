# Ansible Configuration
# Copy this file to .env and customize for your environment

# SSH Configuration
ANSIBLE_USER=ansible
SSH_PRIVATE_KEY_PATH=~/.ssh/id_rsa
SSH_PUBLIC_KEY_PATH=~/.ssh/id_rsa.pub

# Vault Configuration
VAULT_ADDR=https://test.ru
VAULT_ROLE_ID=token
VAULT_SECRET_ID=id

